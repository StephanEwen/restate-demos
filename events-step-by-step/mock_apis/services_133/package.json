{
  "name": "restate-ts-template",
  "version": "0.0.1",
  "description": "Template for JavaScript/TypeScript services running with Restate (https://github.com/restatedev/) ",
  "main": "app.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc --noEmitOnError",
    "app": "node ./dist/app.js",
    "app-dev": "ts-node-dev --watch ./src --transpile-only ./src/app.ts"
  },
  "dependencies": {
    "@restatedev/restate-sdk": "1.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
